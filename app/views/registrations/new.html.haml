%h2= t("devise.registrations.link")
- form_for resource_name, resource, :url => registration_path(resource_name) do |f|
  = f.error_messages
  %p= f.label :email, t(:email).capitalize
  %p= f.text_field :email
  %p= f.label :password, t(:password).capitalize
  %p= f.password_field :password
  %p= f.label :password_confirmation, t(:password_confirmation).capitalize
  %p= f.password_field :password_confirmation
  %p= f.submit t(:sign_up)
= render :partial => "shared/devise_links"
