%h2= t('devise.passwords.link')
- form_for resource_name, resource, :url => password_path(resource_name) do |f|
  = f.error_messages
  %p= f.label :email, t(:your_email).capitalize
  %p= f.text_field :email
  %p= f.submit t('devise.passwords.send_button').capitalize
= render :partial => "shared/devise_links"
